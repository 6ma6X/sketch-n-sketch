
(def foo (\(x y) [(+ x y) (- x y) (* x y) (/ x y)]))

(def multiply3 (\(x y z) (* x (* y z))))

(def max (\(x y)
  (if (< x y) y x)))

(def main
  (draw [
    (show (foo 3 4))
  ]))
